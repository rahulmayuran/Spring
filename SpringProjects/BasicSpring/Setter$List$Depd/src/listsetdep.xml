<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  

<bean id="listsdep" class="setter.listdep.Employee">
<property name="id" value="332"/>
<property name="name" value="Abdullah"/>
<property name="sal_deduction">
	<list>
		<ref bean="salaryone"/>
		<ref bean="salarytwo"/>
	</list>
</property>
</bean>

<bean id="salaryone" class="setter.listdep.Salary">
<property name="hra" value="3500"/>
<property name="bp" value="17000"/>
<property name="tax" value="1400"/>
</bean>

<bean id="salarytwo" class="setter.listdep.Salary">
<property name="hra" value="4500"/>
<property name="bp" value="12000"/>
<property name="tax" value="1800"/>

</bean>
</beans>