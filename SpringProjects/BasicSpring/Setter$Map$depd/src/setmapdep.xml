<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

<bean id="quest" class="setter.mapdepd.Question">
<property name="name" value="OOP Concepts"/>
<property name="id" value="434"/>
<property name="answers">
	<map>
		<entry key-ref="user1" value-ref="ans1"/>
		<entry key-ref="user2" value-ref="ans2"/>
	</map>
</property>
</bean>

<bean id="user1" class="setter.mapdepd.User">
<property name="name" value="Khadim"/>
<property name="id" value="353"/>
<property name="phoneno" value="3890247284"/>
</bean>

<bean id="user2" class="setter.mapdepd.User">
<property name="name" value="Ishaan"/>
<property name="id" value="832"/>
<property name="phoneno" value="91253895131"/>
</bean>

<bean id="ans1" class="setter.mapdepd.Answer">
<property name="answer" value="Polymorphism"></property>
<property name="id" value="353"></property>
<property name="postedDate" value="11/30/1904 21:30:59"></property>
</bean>

<bean id="ans2" class="setter.mapdepd.Answer">
<property name="answer" value="Encapsulation"></property>
<property name="id" value="832"></property>
<property name="postedDate" value="12/06/1965 12:23:33"></property>
</bean>

</beans>